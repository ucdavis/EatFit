.style1 {color: #0066CC; font-weight:bold; letter-spacing: -0.5px;}

.recipetable  {
	text-align: center;
}

.recipetable td {
	background-repeat:no-repeat;
	width: 137px;
	height: 151px;
	line-height: 1em;
	font-size: 12px;
}

.recipetable2 td {
	background-repeat:no-repeat;
	height: 182px;
	line-height: 1em;
	font-size: 12px;
}

.style2 {
	color: #cc0000;
	font-weight: bold;
	font-size: 1.6em;
}

.menulist {
	list-style: none;
	margin: 0;
	padding: 0;
	border: none;
	line-height:0;
}

.menulist li.eatinganalysis{ height:23px;}
.menulist li.games{ height:21px;}
.menulist li.links{ height:20px;}
.menulist li.recipes{ height:19px;}
.menulist li.challengetips{ height:20px;}
.menulist li.stories{ height:20px;}
.menulist li.tips{ height:23px;}

.tablestyle {
	background-color:#eef7fc;
	border: solid 1px #7ca8bf;
}

.tablestyle2 {
	background-color:#e5f1db;
	border: solid 1px #abc299;
}
	
.botbord {
	border-bottom: solid 1px #7ca8bf;
}

.lefttblstyle 
{
	width: 155px; 
	border-bottom: solid 1px #7ca8bf; 
	border-right: solid 1px #7ca8bf; 
	background-color: #EEEEEE;
}

/*-- For the Modal Popup -- */
.modalBackground {
	background-color:#111111;
	filter:alpha(opacity=80);
	opacity:0.8;
}


.modalPopup {
	background-color:#D5DBE1;
	border-width:3px;
	border-style:solid;
	border-color:Gray;
	padding:3px;
	width:250px;
}

.headfont {
	font-size: 18px;
	letter-spacing: -0.5px;
}
	