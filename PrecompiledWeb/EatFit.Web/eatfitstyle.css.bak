/* CSS Document */

body {
	margin-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	font-family:Verdana, Arial, Helvetica, sans-serif;
	font-size: 0.8em;
}

a:link {
	color: #006699; font-weight: bold
}
a:visited {
	color: #006699; font-weight: bold
}
a:hover {
	color: #33ccff; text-decoration:none;  font-weight: bold
}
a:active {
	color: #33ccff; text-decoration:none;  font-weight: bold
}

#wrapper {
	background-image:url(images/greenlines.gif);
	background-repeat: repeat-x;
	width: 700px;
	margin-left: auto;
	margin-right: auto;
}

.box { 
  background: #FFCCCC; 
}
.boxtop { 
  background: url(images/ne.gif) no-repeat top right; 
}
.boxtop div { 
  height: 10px; 
  background: url(images/nw.gif) no-repeat top left; 
}
.boxbottom { 
  background: url(images/se.gif) no-repeat bottom right; 
}
.boxbottom div { 
  height: 10px; 
  background: url(images/sw.gif) no-repeat bottom left; 
}
.boxcontent {
  padding: 0px 10px 0px 10px;
  line-height: 1.5em;
}

.box2 { 
  background: #DBEDA8; 
}
.boxtop2 { 
  background: url(images/ne2.gif) no-repeat top right; 
}
.boxtop2 div { 
